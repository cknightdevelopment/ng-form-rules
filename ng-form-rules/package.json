{
  "name": "ng-form-rules",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve ng-form-rules-samples",
    "build": "ng build ng-form-rules",
    "build:prod": "ng build ng-form-rules --prod",
    "build:npm": "ng build ng-form-rules --prod && copyfiles ../README.md ./dist/ng-form-rules/README.md && echo README copied successfully",
    "test": "ng test ng-form-rules --browsers ChromeNoSandbox --source-manp false",
    "test:once": "ng test ng-form-rules --watch=false --browsers ChromeNoSandbox --progress false --code-coverage",
    "test:ci": "ng test ng-form-rules --watch=false --browsers ChromeNoSandbox --progress false --code-coverage",
    "lint": "ng lint ng-form-rules --type-check --format stylish",
    "pre-push": "npm run lint && npm run test:once && npm run build:prod",
    "sme": "./node_modules/.bin/source-map-explorer"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.2",
    "@angular/common": "6.0.2",
    "@angular/compiler": "6.0.2",
    "@angular/core": "6.0.2",
    "@angular/forms": "6.0.2",
    "@angular/http": "6.0.2",
    "@angular/platform-browser": "6.0.2",
    "@angular/platform-browser-dynamic": "6.0.2",
    "@angular/router": "6.0.2",
    "bootstrap": "^4.1.1",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "rxjs": "^6.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "6.0.2",
    "@angular-devkit/build-angular": "~0.6.1",
    "typescript": "2.7.2",
    "@angular-devkit/build-ng-packagr": "~0.6.1",
    "@angular/cli": "^6.0.1",
    "@angular/language-service": "6.0.2",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-coveralls": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "protractor": "~5.1.2",
    "source-map-explorer": "^1.5.0",
    "ts-node": "~4.1.0",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1"
  }
}
