{
  "name": "lib-test",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ng-form-rules",
    "build:prod": "ng build ng-form-rules --prod",
    "build:npm": "ng build ng-form-rules --prod && copyfiles ../README.md ./dist/ng-form-rules/README.md && echo README copied successfully",
    "test": "ng test ng-form-rules --browsers ChromeNoSandbox --source-map false",
    "test:once": "ng test ng-form-rules --watch=false --browsers ChromeNoSandbox --progress false --code-coverage",
    "test:ci": "ng test ng-form-rules --watch=false --browsers ChromeNoSandbox --progress false --code-coverage",
    "lint": "ng lint ng-form-rules",
    "prepush": "npm run lint && npm run test:once && npm run build:prod",
    "ci": "npm run lint && npm run test:ci && npm run build:prod",
    "sme": "./node_modules/.bin/source-map-explorer"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.3",
    "@angular-eslint/builder": "16.0.3",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/schematics": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "~16.0.3",
    "@angular/compiler-cli": "^16.0.0",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "eslint": "^8.40.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-coveralls": "^2.1.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^16.0.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "~5.0.2"
  }
}
